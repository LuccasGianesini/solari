<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <AssemblyName>Solari.Io</AssemblyName>
    <Authors>Lauth</Authors>
    <Title>Solari.Io</Title>
    <PackageId>Solari.Io</PackageId>
    <PackageDescription>Health Checks</PackageDescription>
    <PackageTags>Extensions;DotnetCore;C#;Vault</PackageTags>
    <TargetFramework>net5.0</TargetFramework>
    <LangVersion>9</LangVersion>
    <RootNamespace>Solari.Io</RootNamespace>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <repositoryUrl>https://github.com/LuccasGianesini/solari</repositoryUrl>
    <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\LICENSE.txt" Pack="true" PackagePath="LICENSE.txt"/>
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Solari.Io.Abstractions\Solari.Io.Abstractions.csproj"/>
  </ItemGroup>

</Project>
