<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <AssemblyName>Solari.Triton</AssemblyName>
    <Authors>Lauth</Authors>
    <Title>Solari.Triton</Title>
    <PackageId>Solari.Triton</PackageId>
    <PackageDescription>Fluent JSON.Net port to netcoreapp5.0</PackageDescription>
    <PackageTags>C#; FluentJsonNet; DotNetCore; Solari; Triton;</PackageTags>
    <TargetFramework>net5.0</TargetFramework>
    <LangVersion>9</LangVersion>
    <RootNamespace>Solari.Triton</RootNamespace>
    <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
    <repositoryUrl>https://github.com/LuccasGianesini/solari</repositoryUrl>

  </PropertyGroup>
  <ItemGroup>
    <None Include="..\..\LICENSE.txt" Pack="true" PackagePath="LICENSE.txt" />
  </ItemGroup>
  <ItemGroup>
    <PackageReference Include="Newtonsoft.Json" Version="12.0.3" />
  </ItemGroup>
  <ItemGroup>
    <ProjectReference Include="..\Solari.Sol\Solari.Sol.csproj" />
  </ItemGroup>

</Project>
