<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <AssemblyName>Solari.Oberon</AssemblyName>
        <Authors>Lauth</Authors>
        <Title>Solari.Oberon</Title>
        <PackageId>Solari.Oberon</PackageId>
        <PackageDescription>Ridis</PackageDescription>
        <PackageTags>C#; DotnetCore; Redis</PackageTags>
        <LangVersion>8</LangVersion>
        <RootNamespace>Solari.Oberon</RootNamespace>
        <repositoryUrl>https://github.com/LuccasGianesini/solari</repositoryUrl>
        <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
        <TargetFrameworks>netcoreapp2.2;netcoreapp3.0;netcoreapp3.1</TargetFrameworks>
    </PropertyGroup>
    <ItemGroup>
        <None Include="..\..\LICENSE.txt" Pack="true" PackagePath="LICENSE.txt" />
    </ItemGroup>
    <ItemGroup>
      <PackageReference Include="Microsoft.Extensions.Caching.Abstractions" Version="3.1.1" />
      <PackageReference Include="Microsoft.Extensions.Caching.Redis" Version="2.2.0" />
    </ItemGroup>
    <ItemGroup>
      <ProjectReference Include="..\Solari.Sol\Solari.Sol.csproj" />
    </ItemGroup>

</Project>
