<Project Sdk="Microsoft.NET.Sdk">

    <PropertyGroup>
        <AssemblyName>Solari.Rhea</AssemblyName>
        <Authors>Lauth</Authors>
        <Title>Solari.Rhea</Title>
        <PackageId>Solari.Rhea</PackageId>
        <PackageDescription>Helath Check library</PackageDescription>
        <PackageTags>Tools;Classes;DotnetCore;C#;Helth Checks</PackageTags>
        <LangVersion>8</LangVersion>
        <RootNamespace>Solari.Rhea</RootNamespace>
        <PackageLicenseFile>LICENSE.txt</PackageLicenseFile>
        <repositoryUrl>https://github.com/LuccasGianesini/solari</repositoryUrl>
        <AllowUnsafeBlocks>true</AllowUnsafeBlocks>
        <TargetFramework>netcoreapp3.1</TargetFramework>
    </PropertyGroup>

    <ItemGroup>
        <None Include="..\..\LICENSE.txt" Pack="true" PackagePath="LICENSE.txt" />
    </ItemGroup>
    
</Project>
