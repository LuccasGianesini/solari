{
  "App": {
    "ApplicationName": "solari-samples-webapi",
    "ApplicationVersion": "1.0.0"
  },
  "Callisto": {
    "Database": "solari-samples",
    "Servers": [
      "localhost:27017"
    ]
  },
  "Deimos": {
    "TracingEnabled": true,
    "UseElasticApm": true,
    "UseCorrelationId": true,
    "Elastic": {
      "Url": "http://172.16.3.3:32514",
      "Subscribers": {
        "ApsNet": true,
        "Http": true
      }
    }
  },
  "Titan": {
    "UseConsole": true,
    "UseElk": true,
    "DefaultLevel": "debug",
    "LogLevelRestriction": "warning",
    "Elk": {
      "Url": "http://172.16.3.3:30769"
    },
    "Overrides": {
      "System": "debug",
      "MicrosoftHostingLifetime": "debug",
      "Microsoft": "debug",
      "Custom": [
        "Elastic.Apm:Debug"
      ]
    }
  }
}

