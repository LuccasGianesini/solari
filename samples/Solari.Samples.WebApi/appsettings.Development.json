{
  "SolariApp": {
    "ApplicationName": "solari-samples-webapi",
    "ApplicationVersion": "1.0.0"
  },
  "Vanth": {
    "UseFluentValidation": true
  },
  "Callisto": {
    "Database": "solari-samples",
    "Servers": [
      "localhost:27017"
    ]
  },
  "Deimos": {
    "TracingEnabled": true,
    "UseElasticApm": true,
    "UseCorrelationId": true,
    "Elastic": {
      "Url": "http://172.16.3.3:32514",
      "Subscribers": {
        "ApsNet": true,
        "Http": true
      }
    }
  },
  "Titan": {
    "UseConsole": true,
    "UseElk": true,
    "DefaultLevel": "information",
    "LogLevelRestriction": "information",
    "Elk": {
      "Url": "http://172.16.3.3:30769"
    },
    "Overrides": {
      "System": "Warning",
      "MicrosoftHostingLifetime": "Warning",
      "Microsoft": "Warning",
      "Custom": [
        "Elastic.Apm:Warning"
      ]
    }
  },
  "rabbitMq": {
    "connectionName": "solari_samples_webapi",
    "messageProcessor": {
      "enabled": true,
      "type": "redis",
      "messageExpirySeconds": 300
    },
    "retries": 3,
    "retryInterval": 2,
    "conventionsCasing": "snakeCase",
    "logger": {
      "enabled": true
    },
    "username": "guest",
    "password": "guest",
    "virtualHost": "/",
    "port": 5672,
    "hostnames": [
      "localhost"
    ],
    "requestTimeout": "00:00:10",
    "publishConfirmTimeout": "00:00:01",
    "recoveryInterval": "00:00:10",
    "persistentDeliveryMode": true,
    "autoCloseConnection": true,
    "automaticRecovery": true,
    "topologyRecovery": true,
    "exchange": {
      "declare": true,
      "durable": true,
      "autoDelete": false,
      "type": "topic",
      "name": "solari.samples"
    },
    "queue": {
      "declare": true,
      "durable": true,
      "exclusive": false,
      "autoDelete": false
    },
    "context": {
      "enabled": true,
      "header": "message_context"
    },
    "spanContextHeader": "span_context"
  }
}

