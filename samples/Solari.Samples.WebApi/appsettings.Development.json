{
  "Application": {
    "ApplicationName": "solari-samples-webapi",
    "ApplicationVersion": "1.0.0"
  },
  "Vanth": {
    "UseFluentValidation": true
  },
  "Callisto": {
    "Database": "solari-samples",
    "Servers": [
      "localhost:27017"
    ]
  },
  "Deimos": {
    "TracingEnabled": true,
    "UseJaeger": true,
    "UseElasticApm": false,
    "UseCorrelationId": true,
    "Http": {
      "UseMiddleware": true
    },
    "Elastic": {
      "Url": "http://127.0.0.1:9200",
      "Subscribers": {
        "ApsNet": true,
        "Http": true
      }
    },
    "Jaeger": {
      "UdpHost": "127.0.0.1",
      "UpdPort": 6371
    }
  },
  "Titan": {
    "UseConsole": true,
    "UseElk": true,
    "DefaultLevel": "debug",
    "LogLevelRestriction": "information",
    "Elk": {
      "Url": "http://127.0.0.1:9200"
    },
    "Overrides": {
      "System": "Warning",
      "MicrosoftHostingLifetime": "Warning",
      "Microsoft": "Warning",
      "Custom": [
        "Elastic.Apm:Warning"
      ]
    }
  },
  "Oberon": {
    "connectionString": "localhost",
    "instance": "solari"
  },
  "Miranda": {
    "Hostnames": [
      "localhost"
    ],
    "Port": 15672,
    "PublishConfirmTimeout": "s10",
    "Exchange": {
      "AutoDelete": false,
      "Durable": true,
      "Type": "topic"
    },
    "Queue": {
      "AutoDelete": false,
      "Durable": true,
      "Exclusive": false
    },
    "Username": "guest",
    "password": "guest",
    "VirtualHost": "/",
    "TopologyRecovery": true,
    "RetryInterval": "s3",
    "PersistentDeliveryMode": true 
  }
}

