{
  "App": {
    "ApplicationName": "solari-samples.webapi",
    "ApplicationVersion": "1.0.0"
  },
  "Callisto": {
    "Database": "solari-samples",
    "Servers": [
      "localhost:27017"
    ]
  },
  "Titan": {
    "UseConsole": true,
    "UseElk": true,
    "DefaultLevel": "debug",
    "LogLevelRestriction": "warning",
    "Elk": {
      "Url": "http://localhost:9200"
    },
    "Overrides":
    {
      "System": "debug",
      "MicrosoftHostingLifetime": "debug",
      "Microsoft": "debug",
      "Custom": ["Elastic.Apm:Debug"]
    }
  } 
}

