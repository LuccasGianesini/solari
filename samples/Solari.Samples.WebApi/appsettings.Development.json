{
  "Application": {
    "ApplicationName": "solari-samples-webapi",
    "ApplicationVersion": "1.0.0"
  },
  "Vanth": {
    "UseFluentValidation": true
  },
  "Callisto": {
    "Database": "solari-samples",
    "Servers": [
      "192.168.120.10:27017"
    ]
  },
  "Deimos": {
    "TracingEnabled": true,
    "UseJaeger": true,
    "UseCorrelationId": true,
    "Http": {
      "UseMiddleware": true
    },
    "Jaeger": {
      "UdpHost": "192.168.120.10",
      "UpdPort": 6831
    }
  },
  "Titan": {
    "UseConsole": true,
    "UseElk": true,
    "DefaultLevel": "debug",
    "LogLevelRestriction": "information",
    "Elk": {
      "Url": "http://192.168.120.10:9200"
    },
    "Overrides": {
      "System": "Warning",
      "MicrosoftHostingLifetime": "Warning",
      "Microsoft": "Warning"
    }
  },
  "Oberon": {
    "connectionString": "localhost",
    "instance": "solari"
  },
  "Miranda": {
    "Hostnames": [
      "192.168.120.10"
    ],
    "Port": 5672,
    "PublishConfirmTimeout": "s10",
    "Exchange": {
      "AutoDelete": false,
      "Durable": true,
      "Type": "topic"
    },
    "Queue": {
      "AutoDelete": false,
      "Durable": true,
      "Exclusive": false
    },
    "Username": "solari",
    "Password": "solari123456",
    "VirtualHost": "/",
    "TopologyRecovery": true,
    "RetryInterval": "s3",
    "PersistentDeliveryMode": true
  }
}

